{
  "name": "JSON Test Assistant",
  "description": "Testing JSON format support",
  "version": "1.0",
  "head": {
    "model": "gpt-4",
    "provider": "openai",
    "system_prompt": "You are a JSON test assistant.",
    "temperature": 0.5,
    "max_tokens": 1500
  },
  "arms": [
    {
      "name": "calculator",
      "type": "python_eval",
      "config": {
        "safe_mode": true
      }
    }
  ],
  "legs": {
    "execution_mode": "single_agent"
  },
  "heart": {
    "memory": {
      "type": "conversation",
      "max_messages": 10
    }
  },
  "spine": {
    "max_tool_calls": 5,
    "timeout_seconds": 30
  }
}
